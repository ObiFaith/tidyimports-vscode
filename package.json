{
  "name": "tidyimports",
  "displayName": "TidyImports",
  "description": "Automatically organize and format your JavaScript/TypeScript imports on save",
  "version": "1.0.0",
  "publisher": "FaithDev",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Formatters",
    "Other"
  ],
  "keywords": [
    "import",
    "organize",
    "sort",
    "format",
    "typescript",
    "javascript",
    "jsx",
    "tsx"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tidyimports.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ObiFaith/tidyimports-vscode"
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "npm run check-types && npm run lint && node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "package": "npm run check-types && npm run lint && node esbuild.js --production",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "check-types": "tsc --noEmit",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "bugs": {
    "url": "https://github.com/ObiFaith/tidyimports-vscode/issues"
  },
  "homepage": "https://github.com/ObiFaith/tidyimports-vscode#readme",
  "license": "MIT",
  "devDependencies": {
    "@babel/generator": "^7.28.5",
    "@babel/parser": "^7.28.5",
    "@babel/traverse": "^7.28.5",
    "@babel/types": "^7.28.5",
    "@types/babel__core": "^7.20.5",
    "@types/babel__generator": "^7.27.0",
    "@types/babel__parser": "^7.0.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.0.3",
    "@types/vscode": "^1.54.0",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "@vscode/test-electron": "^2.5.2",
    "esbuild": "^0.27.2",
    "typescript": "^5.9.3"
  }
}
